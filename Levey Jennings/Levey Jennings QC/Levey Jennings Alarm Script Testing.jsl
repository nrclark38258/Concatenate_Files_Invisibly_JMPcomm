numIndex = 1;
biv = Control Chart(
	Alarm Script(
		Try( Is Scriptable( failTest ), failTest = {} );
		Try(name = report(biv[numIndex])[outlinebox(1)]<<get title());
		Insert Into( failTest, name);
		Write(
			Substitute(
					"Out of Control for test ^QCTEST in column ^QCCOL in sample ^QCSAMPLE",
				"^QCTEST", Char( qc_test ),
				"^QCCOL", qc_col,
				"^QCSAMPLE", Char( qc_sample )
			)
		);
		Write( "" );
		numIndex = numIndex + 1
	),
	KSigma( 3 ),
	Chart Col(
		:Name( "AvgEP(25-45s)" ),
		Levey Jennings(
			Rule 1 2S( 1 ),
			Rule 1 3S( 1 ),
			Rule 2 2S( 1 ),
			Rule R 4S( 1 ),
			Rule 4 1S( 1 ),
			Rule 10 X( 1 )
		)
	),
	By( :Instrument ID )
);
