R Init();
dt = Open("CRP panel data_no30.jmp", invisible);
R Send( dt ); // Sends the opened data table represented by dt to R;
//R Submit( "print(dt)" );

R submit("CRP <- dt");
R submit("X = CRP[,1]");
R submit("Y = CRP[,2]");

//R submit("print (Y)");

R submit ("dualExpFit = nls((Y ~ ((a*exp(b*X)) + (c*exp(d*X)) + e)), start = list(a = 0.01, b = 9, c = 3, d = -3, e = 1))");
//R submit ("print(summary(cubefit))");
R submit ("vals = summary(dualExpFit)$coefficients[,1]");
R submit ("print(vals)");

R Term();
