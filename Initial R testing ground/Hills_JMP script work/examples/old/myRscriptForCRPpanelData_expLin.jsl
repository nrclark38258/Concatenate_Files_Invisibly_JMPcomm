R Init();
dt = Open("CRP panel data_no30.jmp", invisible);
R Send( dt ); // Sends the opened data table represented by dt to R;
//R Submit( "print(dt)" );

R submit("CRP <- dt");
R submit("X = CRP[,1]");
R submit("Y = CRP[,2]");

//R submit("print (Y)");

R submit ("expLinFit = nls((Y ~ I(a*exp(b*X)) + (c*X) + d), start = list(a=0.08, b=7, c=0.5, d=-0.5))");
//R submit ("print(summary(cubefit))");
R submit ("vals = summary(expLinFit)$coefficients[,1]");
R submit ("print(vals)");

R Term();
